docker-compose run web python manage.py collectstatic --noinput
docker-compose run web python manage.py migrate
docker-compose run web python manage.py createsuperuser
docker-compose down
docker-compose up -d



# After docker-compose run execute this commands
docker compose exec web python manage.py migrate [show the database connection.]
docker compose exec web python manage.py showmigrations [show the total files.]

